<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.train.dao.AdminMapper">
   <select id="selectAdmin" resultType="int" parameterType="String">
        select count(*) from admin where adminName=#{adminName} and adminPwd=#{adminPwd}
   </select>
</mapper>